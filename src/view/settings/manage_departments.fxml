<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="393.0" prefWidth="297.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
      <TableView fx:id="tblDepartments" maxWidth="1.7976931348623157E308">
         <placeholder>
            <Label>No departments found</Label>
         </placeholder>
         <columns>
            <TableColumn fx:id="tblDepartmentsColId" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="tblDepartmentsColCategory" prefWidth="200.0" text="Category" />
         </columns>
      </TableView>
      <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
         <Button fx:id="btnNew" mnemonicParsing="false" text="New" />
         <Button fx:id="btnDelete" disable="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Delete" />
      </HBox>
      <HBox alignment="TOP_RIGHT" maxWidth="565.0">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
         <Button fx:id="btnClose" mnemonicParsing="false" text="Close" />
      </HBox>
   </VBox>
</AnchorPane>
