<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="160.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="150.0" />
            <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="80.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="150.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="-Infinity" prefHeight="40.0" valignment="CENTER" vgrow="SOMETIMES" />
            <RowConstraints minHeight="-Infinity" prefHeight="40.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="-Infinity" prefHeight="40.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="-Infinity" prefHeight="40.0" vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
        <Label text="Username:" />
        <TextField fx:id="txtUsername" GridPane.columnIndex="1" />
        <Label text="Password:" GridPane.columnIndex="2" GridPane.rowIndex="0">
            <GridPane.margin>
                <Insets left="20.0" />
            </GridPane.margin>
        </Label>
        <PasswordField fx:id="txtPassword" GridPane.columnIndex="3" GridPane.rowIndex="0" />
        <Label text="Department:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <ChoiceBox fx:id="choiceDepartment" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Label text="Shift:" GridPane.columnIndex="2" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="20.0" />
            </GridPane.margin>
        </Label>
        <ChoiceBox fx:id="choiceShift" maxWidth="1.7976931348623157E308" GridPane.columnIndex="3" GridPane.rowIndex="1" />
        <Label text="Permission Level:" GridPane.columnIndex="0" GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets />
            </GridPane.margin>
        </Label>
        <ChoiceBox fx:id="choicePermission" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <CheckBox fx:id="checkPasswordChange" text="Require password change on login" GridPane.columnSpan="2" GridPane.rowIndex="3" />
        <Button fx:id="btnDelete" disable="true" mnemonicParsing="false" text="Delete" GridPane.columnIndex="0" GridPane.rowIndex="4" />
        <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="4">
            <Button fx:id="btnCancel" mnemonicParsing="false" text="Cancel" />
            <Button fx:id="btnSave" layoutX="119.0" layoutY="13.0" mnemonicParsing="false" text="Save">
                <HBox.margin>
                    <Insets left="10.0" />
                </HBox.margin>
            </Button>
        </HBox>
    </GridPane>
</AnchorPane>
