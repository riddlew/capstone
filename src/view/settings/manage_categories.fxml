<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="393.0" prefWidth="585.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox>
               <children>
                  <VBox maxWidth="1.7976931348623157E308">
                     <children>
                        <TableView fx:id="tblCategories" maxWidth="1.7976931348623157E308">
                            <placeholder>
                                <Label>No categories found</Label>
                            </placeholder>
                          <columns>
                            <TableColumn fx:id="tblCategoriesColId" prefWidth="75.0" text="ID" />
                              <TableColumn fx:id="tblCategoriesColCategory" prefWidth="200.0" text="Category" />
                          </columns>
                        </TableView>
                        <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                           <children>
                              <Button fx:id="btnNewCategory" mnemonicParsing="false" text="New" />
                              <Button fx:id="btnDeleteCategory" disable="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Delete" />
                           </children>
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </HBox>
                     </children>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox maxWidth="1.7976931348623157E308">
                     <children>
                        <TableView fx:id="tblSubcategories" maxWidth="1.7976931348623157E308">
                           <placeholder>
                              <Label>No subcategories found</Label>
                           </placeholder>
                           <columns>
                              <TableColumn fx:id="tblSubcategoriesColId" prefWidth="75.0" text="ID" />
                              <TableColumn fx:id="tblSubcategoriesColSubcategory" prefWidth="200.0" text="Subcategory" />
                           </columns>
                        </TableView>
                        <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                           <children>
                              <Button fx:id="btnNewSubcategory" disable="true" mnemonicParsing="false" text="New" />
                              <Button fx:id="btnDeleteSubcategory" disable="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Delete" />
                           </children>
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="TOP_RIGHT" maxWidth="565.0">
               <children>
                  <Button fx:id="btnClose" mnemonicParsing="false" text="Close" />
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
